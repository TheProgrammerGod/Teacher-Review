<?xml version="1.0" encoding="utf-8"?>

<androidx.cardview.widget.CardView xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto"
    xmlns:tools="http://schemas.android.com/tools"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:layout_marginBottom="23dp"
    android:backgroundTint="@color/dark_grey"
    android:elevation="4dp"
    app:cardCornerRadius="16dp">

    <androidx.constraintlayout.widget.ConstraintLayout
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <ImageView
            android:id="@+id/ivProfilePic_Item_Review_History"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="9dp"
            android:layout_marginTop="14dp"
            android:scaleType="centerCrop"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/test_image_icon" />

        <TextView
            android:id="@+id/tvTeacherName_Item_Review_History"
            style="@style/TextViewYellow_20sp"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="14dp"
            android:layout_marginTop="14dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/ivProfilePic_Item_Review_History"
            app:layout_constraintTop_toTopOf="parent"
            app:layout_constraintVertical_bias="0.0"
            tools:text="Dr. BKL ADD" />

        <ImageView
            android:id="@+id/ivStar1_Item_Review_History"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="14dp"
            android:layout_marginTop="6dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toEndOf="@+id/ivProfilePic_Item_Review_History"
            app:layout_constraintTop_toBottomOf="@+id/tvTeacherName_Item_Review_History"
            app:layout_constraintVertical_bias="0.0"
            app:srcCompat="@drawable/zero_star_icon" />

        <ImageView
            android:id="@+id/ivStar2_Item_Review_History"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            app:layout_constraintBottom_toBottomOf="@+id/ivStar1_Item_Review_History"
            app:layout_constraintStart_toEndOf="@+id/ivStar1_Item_Review_History"
            app:layout_constraintTop_toTopOf="@+id/ivStar1_Item_Review_History"
            app:srcCompat="@drawable/zero_star_icon" />

        <ImageView
            android:id="@+id/ivStar3_Item_Review_History"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            app:layout_constraintBottom_toBottomOf="@+id/ivStar2_Item_Review_History"
            app:layout_constraintStart_toEndOf="@+id/ivStar2_Item_Review_History"
            app:layout_constraintTop_toTopOf="@+id/ivStar2_Item_Review_History"
            app:srcCompat="@drawable/zero_star_icon" />

        <ImageView
            android:id="@+id/ivStar4_Item_Review_History"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            app:layout_constraintBottom_toBottomOf="@+id/ivStar3_Item_Review_History"
            app:layout_constraintStart_toEndOf="@+id/ivStar3_Item_Review_History"
            app:layout_constraintTop_toTopOf="@+id/ivStar3_Item_Review_History"
            app:srcCompat="@drawable/zero_star_icon" />

        <ImageView
            android:id="@+id/ivStar5_Item_Review_History"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:layout_marginStart="6dp"
            app:layout_constraintBottom_toBottomOf="@+id/ivStar4_Item_Review_History"
            app:layout_constraintStart_toEndOf="@+id/ivStar4_Item_Review_History"
            app:layout_constraintTop_toTopOf="@+id/ivStar4_Item_Review_History"
            app:srcCompat="@drawable/zero_star_icon" />

        <TextView
            android:id="@+id/tvReview_Item_Review_History"
            style="@style/TextViewWhite_18sp"
            android:layout_width="0dp"
            android:layout_height="wrap_content"
            android:layout_marginStart="60dp"
            android:layout_marginTop="13dp"
            android:layout_marginEnd="8dp"
            android:layout_marginBottom="9dp"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/ivStar1_Item_Review_History"
            app:layout_constraintVertical_bias="0.0"
            tools:text="Reviews Goes Here" />

        <androidx.appcompat.widget.LinearLayoutCompat
            android:id="@+id/layout_Item_Review_History_Rating"
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_marginStart="25dp"
            android:layout_marginTop="14dp"
            android:layout_marginEnd="20dp"
            android:layout_marginBottom="8dp"
            android:orientation="vertical"
            app:layout_constraintBottom_toBottomOf="parent"
            app:layout_constraintEnd_toEndOf="parent"
            app:layout_constraintHorizontal_bias="0.0"
            app:layout_constraintStart_toStartOf="parent"
            app:layout_constraintTop_toBottomOf="@+id/tvReview_Item_Review_History">

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/teachingRatingLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="12dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/textView7"
                    style="@style/TextViewYellow_20sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/teaching_rating"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    app:layout_constraintBottom_toBottomOf="@+id/textView7"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toEndOf="@+id/textView7"
                    app:layout_constraintTop_toTopOf="@+id/textView7"
                    tools:layout_editor_absoluteX="12dp"
                    tools:layout_editor_absoluteY="12dp">

                    <ImageView
                        android:id="@+id/iv_star1_Item_Review_History_Teaching_Rating"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@drawable/zero_star_icon" />

                    <ImageView
                        android:id="@+id/iv_star2_Item_Review_History_Teaching_Rating"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="11dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/zero_star_icon" />

                    <ImageView
                        android:id="@+id/iv_star3_Item_Review_History_Teaching_Rating"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="11dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/zero_star_icon" />

                    <ImageView
                        android:id="@+id/iv_star4_Item_Review_History_Teaching_Rating"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="11dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/zero_star_icon" />

                    <ImageView
                        android:id="@+id/iv_star5_Item_Review_History_Teaching_Rating"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="11dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/zero_star_icon" />
                </LinearLayout>

                <TextView
                    android:id="@+id/tvReview_Item_Review_History_Teaching_Review"
                    style="@style/TextViewWhite_18sp"
                    android:layout_width="0dp"

                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="13dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView7"
                    tools:text="Teaching Review Goes here" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/marksRatingLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="12dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/textView8"
                    style="@style/TextViewYellow_20sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/marks_rating"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0" />

                <LinearLayout
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    app:layout_constraintBottom_toBottomOf="@+id/textView8"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toEndOf="@+id/textView8"
                    app:layout_constraintTop_toTopOf="@+id/textView8"
                    tools:layout_editor_absoluteX="12dp"
                    tools:layout_editor_absoluteY="12dp">

                    <ImageView
                        android:id="@+id/iv_star1_Item_Review_History_Marks_Rating"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@drawable/zero_star_icon" />

                    <ImageView
                        android:id="@+id/iv_star2_Item_Review_History_Marks_Rating"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="11dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/zero_star_icon" />

                    <ImageView
                        android:id="@+id/iv_star3_Item_Review_History_Marks_Rating"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="11dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/zero_star_icon" />

                    <ImageView
                        android:id="@+id/iv_star4_Item_Review_History_Marks_Rating"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="11dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/zero_star_icon" />

                    <ImageView
                        android:id="@+id/iv_star5_Item_Review_History_Marks_Rating"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="11dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/zero_star_icon" />
                </LinearLayout>

                <TextView
                    android:id="@+id/tvReview_Item_Review_History_Marks_Review"
                    style="@style/TextViewWhite_18sp"
                    android:layout_width="0dp"

                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="13dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView8"
                    tools:text="Teaching Review Goes here" />
            </androidx.constraintlayout.widget.ConstraintLayout>

            <androidx.constraintlayout.widget.ConstraintLayout
                android:id="@+id/attendanceRatingLayout"
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:padding="12dp"
                app:layout_constraintBottom_toBottomOf="parent"
                app:layout_constraintEnd_toEndOf="parent"
                app:layout_constraintStart_toStartOf="parent"
                app:layout_constraintTop_toTopOf="parent">

                <TextView
                    android:id="@+id/textView9"
                    style="@style/TextViewYellow_20sp"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:text="@string/attendance_rating"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toTopOf="parent"
                    app:layout_constraintVertical_bias="0.0" />

                <LinearLayout
                    android:id="@+id/linearLayout1"
                    android:layout_width="wrap_content"
                    android:layout_height="match_parent"
                    android:orientation="horizontal"
                    app:layout_constraintBottom_toBottomOf="@+id/textView9"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintHorizontal_bias="1.0"
                    app:layout_constraintStart_toEndOf="@+id/textView9"
                    app:layout_constraintTop_toTopOf="@+id/textView9"
                    tools:layout_editor_absoluteX="12dp"
                    tools:layout_editor_absoluteY="12dp">

                    <ImageView
                        android:id="@+id/iv_star1_Item_Review_History_Attendance_Rating"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:scaleType="centerCrop"
                        android:src="@drawable/zero_star_icon" />

                    <ImageView
                        android:id="@+id/iv_star2_Item_Review_History_Attendance_Rating"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="11dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/zero_star_icon" />

                    <ImageView
                        android:id="@+id/iv_star3_Item_Review_History_Attendance_Rating"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="11dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/zero_star_icon" />

                    <ImageView
                        android:id="@+id/iv_star4_Item_Review_History_Attendance_Rating"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="11dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/zero_star_icon" />

                    <ImageView
                        android:id="@+id/iv_star5_Item_Review_History_Attendance_Rating"
                        android:layout_width="wrap_content"
                        android:layout_height="wrap_content"
                        android:layout_marginStart="11dp"
                        android:scaleType="centerCrop"
                        android:src="@drawable/zero_star_icon" />
                </LinearLayout>

                <TextView
                    android:id="@+id/tvReview_Item_Review_History_Attendance_Review"
                    style="@style/TextViewWhite_18sp"
                    android:layout_width="0dp"

                    android:layout_height="wrap_content"
                    android:layout_marginStart="24dp"
                    android:layout_marginTop="13dp"
                    app:layout_constraintBottom_toBottomOf="parent"
                    app:layout_constraintEnd_toEndOf="parent"
                    app:layout_constraintStart_toStartOf="parent"
                    app:layout_constraintTop_toBottomOf="@+id/textView9"
                    tools:text="Teaching Review Goes here" />

            </androidx.constraintlayout.widget.ConstraintLayout>
        </androidx.appcompat.widget.LinearLayoutCompat>

    </androidx.constraintlayout.widget.ConstraintLayout>
</androidx.cardview.widget.CardView>
